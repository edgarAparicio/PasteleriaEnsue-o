@model PasteleriaYummy.ViewModels.CarritoComprasViewModel

@if (Model.DataCarritoCompras.ListaArticulosCarritoCompras.Count > 0)
{
    <li>
        <a>
            <span class="glyphicon glyphicon-shopping-cart"></span>
            <span id="cart-status">
                @Model.DataCarritoCompras.ListaArticulosCarritoCompras.Count
            </span>
        </a>
    </li>
}